#solve with adams method:
f = open('advent_of_code_day_2.txt', 'r')
x = f.readlines()
num_two =[]
num_three=[]
for i in x:
    s2=[]
    s3=[]
    y=0
    while y < len(i):
        if (i.count(i[y])==2):
            s2.append(i[y])
        if (i.count(i[y])==3):
            s3.append(i[y])
        y += 1
    if len(s2) >= 1:
        num_two.append(i)
    if len(s3) >= 1:
        num_three.append(i)
print(len(num_two)*len(num_three)) #checksum
f.close()
